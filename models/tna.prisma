model TNA {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  orderNumber    String
  buyer          String
  style          String
  totalTasks     Int
  completedTasks Int
  percentage     Int
  status         String
  dueDate        DateTime
  currentStage   String
  merchandiser   String
  priority       String
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}